{
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:@typescript-eslint/recommended-requiring-type-checking",
    "prettier/@typescript-eslint",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2017,
  },
  "env": {
    "es6": true
  },
  "rules": {
    "quote-props": "warn"
  }
}